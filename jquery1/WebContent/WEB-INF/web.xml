<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>jquery1</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
	<servlet>
		<description>jQuery Ajax Request</description>
		<display-name>AjaxController</display-name>
		<servlet-name>AjaxController</servlet-name>
		<servlet-class>controller.AjaxController</servlet-class>
	</servlet>
	
	<servlet>
		<description>DeveloperController</description>
		<display-name>DeveloperController</display-name>
		<servlet-name>DeveloperController</servlet-name>
		<servlet-class>controller.DeveloperController</servlet-class>
	</servlet>
</web-app>